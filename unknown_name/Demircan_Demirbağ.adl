info analysis
	title "Search for new phenomena in events with an energetic jet and missing transverse momentum in ğ’‘ ğ’‘ collisions at pğ’” = 13 TeV with the ATLAS detector"
	experiment ATLAS
	sqrtS 13.0
	lumi 139
	arXiv "2102.10874v"

object jets
	take jet
	select pT(jet) > 20
	select abseta(jet) < 2.8

object bjets
	take jets
	select btag(jets) == 1
	select pT(jet) > 30
	select abseta(jet) < 2.5

object eles
	take ele
	select pT(ele) > 7
	select abseta(ele) < 2.47
	select loose(ele) == 1

object jetss # second s stands for second correction # makaleye paralel olsun diye tanÄ±mÄ±mÄ± parÃ§alÄ± yaptÄ±m
	take bjets
	select dR(eles,bjets) >= 0.2 # herhangi elektron ile jet arasÄ±ndaki aÃ§Ä± 0.2den kÃ¼Ã§Ã¼kse jeti at. aÃ§Ä± 0.2den bÃ¼yÃ¼kse jeti al elektronu at
#bu yazÄ±m elektron olmadÄ±ÄŸÄ±nda hata verir mi? her seferinde conditional yazmalÄ± mÄ±yÄ±m?

object elesf # eles final
	take eles
	select dR(eles,bjets) < 0.2 # yukarÄ±daki ÅŸartÄ±n devamÄ±
	reject dR(eles,jetss) [] 0.2 0.4 # kalan jetlere bu aralÄ±k kadar yakÄ±n olan elektronlarÄ± at

object muos
	take muo
	select pT(muo) > 7
	select abseta(muo) < 2.5
	select medium(muo) == 1

object muosf # track'leri ile ilgili ÅŸartlar vardÄ± koymamÄ±ÅŸ oldum
	take muos
	select ALL # hepsini seÃ§ bÃ¶yle denir deÄŸil mi? diÄŸer objeleri Ã§ekmiyor deÄŸil mi?

object taus
	take tau
	select pT(tau) > 10
	select abseta(tau) < 2.5
	select loose(tau) == 1 # bunun anlamÄ±nÄ±n pt>20 ve abseta [] 1.37 1.52 ve tracklerle ilgili ÅŸartlar 
#olduÄŸunu sÃ¶ylÃ¼yor ayrÄ±ca belirtmedim, loose'un anlamÄ±nÄ±n bu olduÄŸuna nereden bakmam gerekiyor


object tausf
	take taus
	reject dR(taus,muos) < 0.2
	reject dR(taus,elesf) < 0.2

object jetsss
	take jetss
	reject dR(tausf,jetss) < 0.2

object phos
	take pho
	select tight(pho) == 1
	select pT(pho) > 10
	select abseta(pho) < 2.37

object phosf
	take phos
	reject dR(phos,elesf) < 0.4
	reject dR(phos,muos) < 0.4

object jetsf
	take jetsss
	reject dR(phosf,jetsss) < 0.4

